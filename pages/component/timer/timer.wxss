/* pages/component/timer/timer.wxss */

.timeWrap{
  width: 100%;
  height: 420rpx;
  position: relative;
  background:azure
 
}
.picker_view_wrapper{
  width: 100%;
  height: 100%;
   /* background: red; */
  box-sizing: border-box;
  padding: 0 140rpx;
  background: #ffffff
}
.picker_view_wrapper .indicator-style{
  height: 70rpx
}
.picker_view_wrapper .picker-view-txt{
  font-size: 50rpx;
  text-align: center;
  line-height: 70rpx;
  height: 70rpx
}
.timeWrap .maohao{
  position: absolute;
  font-size: 60rpx;
  height: 70rpx;
  top: 50%;
  left: 50%;
  line-height: 62rpx;
  transform: translate(-50%,-50%)
}